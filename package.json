{
  "name": "wooster-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "drizzle:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "drizzle:generate:test": "drizzle-kit generate --config=drizzle.test.config.ts",
    "drizzle:migrate": "drizzle-kit push --config=drizzle.config.ts",
    "drizzle:migrate:test": "cross-env NODE_ENV=test drizzle-kit push --config=drizzle.test.config.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "type-check": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest",
    "test:unit": "cross-env NODE_ENV=test jest \"tests/unit/.*\\.test\\.ts$\"",
    "test:integration": "cross-env NODE_ENV=test jest \"tests/integration/.*\\.test\\.ts$\"",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "msw": "^2.6.5",
    "nodemon": "^3.1.5",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/pg": "^8.11.10",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "pino": "^9.5.0",
    "postgres": "^3.4.4",
    "uuid": "^10.0.0"
  }
}
